import java.util.Scanner;

public class Test1 {

	public static void main(String[] args) {

		Scanner inp = new Scanner(System.in);
	    System.out.print("In:");
	    String word = inp.nextLine();
	    //write your code below
	    
	    char[] enter = word;
	    for (int i=0; i<enter.length; i++) {
	    	System.out.println(enter);
	    }
		
	
	
	}
}

				
			
		
		





