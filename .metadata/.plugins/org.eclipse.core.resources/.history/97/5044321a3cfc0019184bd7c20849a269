public class Main
{
	public static void main(String[] args)
	{
		int[][] a = {
				{-5,-2,-3,7},
				{1,-5,-2, 2},
				{1,-2, 3,-4}
			};
			
			
	//   should print -9
		
		int num=0;
		for(int i=0; i<a.length;i++) {
			for(int j=0; j>a[i][1]; j++) {
				num+=a[i][1];
				System.out.print(num);
			}
		}
		
		
		
		
		
		
		
	}	
}

		